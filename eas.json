    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "18.18.0"
    },
    "preview": {
      "distribution": "internal",
      "node": "18.18.0",
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "node": "18.18.0",
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_SETLISTFM_API_KEY": ""
      }
    }
  },
  "submit": {
    "production": {}
  }
}
